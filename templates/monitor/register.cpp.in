#include "@PLUGIN@register.h"
#include "@PLUGIN@builder.h"
#include "monitorpluginfactory.h"
#include "version.h"

void CreateMonitorBuilder()
{
    MonitorPluginFactory::Get()->Register(new @PLUGIN@Builder());
}

wxString GetPluginName()
{
    return "@PLUGIN@";
}

wxString GetPluginVersion()
{
    return wxString::FromAscii(AutoVersion::FULLVERSION_STRING);
}

wxString GetDetails()
{
    return "@DESCRIPTION@";
}
